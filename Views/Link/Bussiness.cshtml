<div class="card">
<div class="container">
    <br>
        <h5 class="dh right-align">މާމިގިލީ ވިޔަފާރީ އާއި ގުޅޭ</h5>
        <hr>
@foreach (var item in  ViewBag.bussines)
{
      <div class="row">
        
        <div class="dh right-align col s12">
            <h5>@item.Title</h5>
            <img src="~/LinkPic/@item.ImgUrl" class="center-align responsive-img" height="484px" width="750px">
            <p style="overflow-wrap: break-word;">
                @item.Text
            </p>
        </div>
         
    </div>
    
}
  
</div>
</div>