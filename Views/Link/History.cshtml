<div class="card">
<div class="container center-align">
    <br>
    <h5 class="dh right-align">މާމިގިލީ ގައި ހުރި މުހިންމު ތަންތަނުގެ މަޢުލޫމާތު</h5>

<hr>
        <div class="row">
            <div class="col s12 center-align">
                     @foreach (var item in  ViewBag.history)
        {
            <h6 class=" dh right-align">
                @item.Title
            </h6>
            <img src="~/LinkPic/@item.ImgUrl" class="center-align"/>
            <p class="dh right-align" style="direction: rtl;text-align: justify;">
                @item.Text
            </p>
            <br>
            <hr>
           
        }
            </div>
   
        </div>

</div>
</div>