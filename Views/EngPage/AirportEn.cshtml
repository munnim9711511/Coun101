<div class="card">
<div class="container center-align">
   <h5 class="left-align">
     Villa Airport
   </h5>
   <hr>
<div class="row">
    <div class="col s12  left-align">
        
        @foreach (var item in  ViewBag.air )
        {
            <h5 class="dh " >@item.Title</h5>
        <hr>
            <img src="~/LinkPic/@item.ImgUrl" class="responsive-img" style="width: 50%">
             <p class="dh" style="direction: rtl;text-align: justify;">
            @item.Text
        </p>
        }
       
    </div>
</div>
</div>
</div>