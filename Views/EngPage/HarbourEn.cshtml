<div class="card">
<div class="container center-align">
    <br>
   <h5 class="left-align">
     Maamigili International Harbour
   </h5>
   <hr>
<div class="row">
    <div class="col s12  left-align">
        
        @foreach (var item in  ViewBag.harbourEn )
        {
            <h5 class="dh " >@item.Title</h5>
        <hr>
            <img src="~/LinkPic/@item.ImgUrl" class="responsive-img" width="700px" height="300px">
             <p  style="text-align: justify; overflow-wrap: break-word;">
            @item.Text
        </p>
        }
       
    </div>
</div>
</div>
</div>